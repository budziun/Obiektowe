// Interfejs MyComparator
public interface MyComparator {
    int compare(int a, int b);
}

// Implementacja klasy TestMyComparator
public class TestMyComparator implements MyComparator {
    @Override
    public int compare(int a, int b) {
        if (a < b) {
            return -1;
        } else if (a > b) {
            return 1;
        } else {
            return 0;
        }
    }

    // Przykład użycia
    public static void main(String[] args) {
        TestMyComparator comparator = new TestMyComparator();
        
        // Testy porównań
        int result1 = comparator.compare(5, 10);  // Powinno zwrócić -1
        int result2 = comparator.compare(20, 10); // Powinno zwrócić 1
        int result3 = comparator.compare(15, 15); // Powinno zwrócić 0
        
        // Wyświetlanie wyników testów
        System.out.println("Result 1: " + result1);
        System.out.println("Result 2: " + result2);
        System.out.println("Result 3: " + result3);
    }
}
